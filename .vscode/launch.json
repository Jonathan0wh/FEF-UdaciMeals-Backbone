{
    "version": "0.2.0",
    "configurations": [{
            "name": "Launch Chrome against localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8000/index.html",
            "webRoot": "${workspaceRoot}",
            "diagnosticLogging": true
        },
        {
            "name": "Launch Firefox",
            "type": "firefox",
            "request": "launch",
            "url": "http://localhost:8000/index.html",
            "webRoot": "${workspaceRoot}"
        }
    ]
}